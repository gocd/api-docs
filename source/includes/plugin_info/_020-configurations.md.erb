<%
    json =  {
  configurations: [
    {
      key: "url",
      metadata: {
        secure: false,
        required: true,
        part_of_identity: false
      }
    }
  ]
}
%>
<%=
describe_sub_object 'The plugin info configurations object', json: json, html_id: 'plugin-info-configurations' do
  key      'String', 'Describes the configuration key'
  metadata 'Object', "Metadata for the configuration property. For plugins of types `scm`, the metadata consists of attributes `secure`, `required` and `part_of_identity`. The attributes for [package-repository](#the-package-repository-configurations-object) and [elastic](#the-elastic-configurations-object) plugins are given below."
end
%>

<%
    repo_json =  {
  configurations: [
      {
        key: "url",
        metadata: {
          secure: false,
          required: true,
          part_of_identity: false,
          display_name: "Display name",
          display_order: 0
        }
      }]
}
%>

<%=
describe_sub_object 'The package repository configurations object', json: repo_json, html_id: 'package-repository-configurations' do
  key      'String', 'Describes the configuration key for a repository configuration property'
  metadata 'Object', "Metadata for the configuration property."
end
%>

