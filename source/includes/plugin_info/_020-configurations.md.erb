<%
    json =  {
  configurations: [
    {
      key: "url",
      metadata: {
        secure: false,
        required: true,
      }
    }
  ]
}
%>
<%=
describe_sub_object 'The plugin info configurations object', json: json, html_id: 'plugin-info-configurations' do
  key      'String', 'Describes the configuration key'
  metadata 'Object', "Metadata for the configuration property. The metadata consists of attributes `secure`, `required`. The attributes for [package-repository](#the-package-repository-configurations-object) and [scm](#the-scm-configurations-object) plugins are given below."
end
%>

<%
    repo_json =  {
  configurations: [
    {
      key: "REPO_URL",
      metadata: {
        secure: false,
        display_order: 0,
        required: true,
        part_of_identity: false,
        display_name: "Repository URL"
      }
    }
  ]
}
%>

<%=
describe_sub_object 'The package repository configurations object', json: repo_json, html_id: 'package-repository-configurations' do
  key      'String', 'Describes the configuration key for a repository configuration property'
  metadata 'Object', "Metadata for the configuration property."
end
%>

<%
    scm_json =  {
  configurations: [
    {
      key: "REPO_URL",
      metadata: {
        secure: false,
        required: true,
        part_of_identity: false,
      }
    }
  ]
}
%>

<%=
describe_sub_object 'The scm configurations object', json: scm_json, html_id: 'scm-configurations' do
  key      'String', 'Describes the configuration key for a repository configuration property'
  metadata 'Object', "Metadata for the configuration property."
end
%>
